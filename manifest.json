{
  "manifest_version": 3,
  "name": "Developer Dashboard",
  "description": "A customized developer dashboard that replaces the new tab page.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage", "background"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
